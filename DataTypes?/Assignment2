using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter a string: ");
        string input = Console.ReadLine();

        int digitCount = 0;
        int alphabetCount = 0;

        foreach (char c in input)
        {
            if (char.IsDigit(c))
            {
                digitCount++;
            }
            else if (char.IsLetter(c))
            {
                alphabetCount++;
            }
        }

        Console.WriteLine($"\nNumber of alphabets: {alphabetCount}");
        Console.WriteLine($"Number of digits: {digitCount}");
    }
}
